<layouts>
	<layout name="root">
		<row>
			<button type="picture" icon="logo_camera"/>
			<button type="textnote" icon="logo_textnote"/>
			<button type="voicerec" icon="logo_voice"/>
		</row>
		<row>
			<button type="page" label="Ograničenja" targetlayout="ogranicenja" icon="maxspeed_icons/100.png"/>
			<button type="page" label="Upozorenja" targetlayout="upozorenja" icon="maxspeed_icons/warning.png"/>
		</row>
	</layout>
	<layout name="ogranicenja">
